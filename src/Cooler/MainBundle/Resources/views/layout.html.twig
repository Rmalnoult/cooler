<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset="utf-8">
    {% block seo %}
    <title>{% block title %}{{ title|default('Cooler')}}{% endblock title %}</title>
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700' rel='stylesheet' type='text/css'>
    <meta name="keywords" content="{{ keywords|default('Cooler')}}" />
    <meta name="description" content="{{description|default('')}}" />
    <meta name="robots" content="index, follow" /> 
    {% endblock %}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {% block css %}
          <link rel="stylesheet" type="text/css" href="{{asset('css/home.css')}}">
          <link rel="stylesheet" type="text/css" href="{{asset('css/camera.css')}}">
          <link rel="stylesheet" type="text/css" href="{{asset('css/forms.css')}}">
          <link rel="stylesheet" type="text/css" href="{{asset('css/forms.css')}}">
    {% endblock css %}

  </head>
  <body style="height:100%" >
  {% block fos_user_content %}
  {% endblock fos_user_content %}
      
  {% block header %}
  {% endblock header %}
        

    {% block content %}

    {% endblock content %}

 
  {% block javascript %}
    <script src="{{asset('js/jquery-2.1.3.min.js')}}"></script>
    <script src="{{asset('js/photoUpload.js')}}"></script>
    
  {% endblock %}  
  </body>
</html>
